{
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/server/index.ts" }
    ],
    "functions": {
      "server/index.ts": {
        "runtime": "vercel-node@canary"
      }
    },
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/dist"
        }
      }
    ]
  }
  