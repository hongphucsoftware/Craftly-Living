{
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/api/index.ts" }
    ],
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      },
      {
        "src": "api/index.ts",
        "use": "@vercel/node"
      }
    ]
  }
  